{
  "name": "Feed",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "feedName": {
      "type": "string",
      "required": true,
      "default": "Feed"
    },
    "feedType": {
      "type": "string",
      "required": true,
      "enum": [
        "user",
        "mylist",
        "search",
        "tags"
      ]
    },
    "query": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "ownerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
